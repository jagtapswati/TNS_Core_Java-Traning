package com.tnsif.methodoverloadingdemo;

public class methodoverloading{

    public static int addition(int a,int b)
    {
        return a+b;

    }
    public static float adddition(float a,int b)
    {
        return a+b;

    }
    public static float addition(int a,float b)
    {
        return a+b;

    }
    public static float addition(float a,float b)
    {
        return a+b;
    }
    public static String adddition(String a, String b)
    {
        return a+b;
    }
}